{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "VITE_API_BASE_URL": "",
      "VITE_CLERK_PUBLISHABLE_KEY": ""
    },
    "build": {
      "env": {
        "VITE_API_BASE_URL": "",
        "VITE_CLERK_PUBLISHABLE_KEY": ""
      }
    }
  }