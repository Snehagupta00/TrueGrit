{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "MONGO_URI": "",
      "PORT": "5000",
      "CLERK_PUBLISHABLE_KEY": "",
      "CLERK_SECRET_KEY": ""
    }
  }